<table class="table table-striped border border-secondary mx-auto my-5" style="width: 1200px;">
  <thead class="table-primary">
    <tr>
      <th scope="col">#</th>
      <th scope="col">Title</th>
      <th scope="col">Content</th>
    </tr>
  </thead>

  <tbody>
    <% @posts.each do |post| %>
    <tr>
      <td><%= post.id %></td>
      <td><%= post.title %></td>
      <td><%= post.content %></td>
    </tr>
    <% end %>
  </tbody>
</table>